<idea-plugin>
  <id>com.mechanicus.theme</id>
  <name>Adeptus Mechanicus Theme</name>
  <vendor>Tech-Priest</vendor>
  <description>Warhammer 40K Adeptus Mechanicus themes - Dark forge world and light parchment versions. Praise the Omnissiah!</description>
  <version>1.0</version>
  
  <idea-version since-build="193.0"/>
  
  <depends>com.intellij.modules.platform</depends>
  
  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="com.mechanicus.theme.dark" path="/theme_dark/Adeptus_Mechanicus_Dark.theme.json"/>
    <themeProvider id="com.mechanicus.theme.light" path="/theme_light/Adeptus_Mechanicus_Light.theme.json"/>
  </extensions>
</idea-plugin>